<h2>Laissez un message</h2>
<form method="POST" action="/contact">
  <textarea name="message"></textarea>
  <input type="hidden" name="_csrf" value="<%= csrfToken %>">
  <button type="submit">Envoyer</button>
</form>

<h3>Messages :</h3>
<ul>
  <% messages.forEach(function(msg) { %>
    <li><%= msg %></li>
  <% }) %>
</ul>
